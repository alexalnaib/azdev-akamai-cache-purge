    "debug": "^4.3.2",
    "edgegrid": "^3.0.8"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.6",
    "@types/q": "^1.5.5",
    "@types/sinon": "^10.0.3",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
