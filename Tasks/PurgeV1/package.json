    "mocha-multi-reporters": "^1.5.1",
    "sinon": "^11.1.2",
    "ts-mock-imports": "^1.3.7",
    "ts-node": "^10.2.1",
    "typemoq": "^2.1.0",
    "typescript": "^4.4.3"
